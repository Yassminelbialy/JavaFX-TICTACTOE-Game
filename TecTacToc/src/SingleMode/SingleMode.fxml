<AnchorPane id="AnchorPane" prefHeight="576.0" prefWidth="908.0" style="-fx-background-color: #0b3660;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="SingleMode.SingleModeController">
